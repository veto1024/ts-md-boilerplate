  "unpkg": "dist/index.min.js",
  "description": "Boilerplate code for a Typescript/Material Design project with Formik helpers.",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.0",
