{
  "name": "ts-bs-boilerplate",
  "version": "0.1.1",
  "author": "Jonathan Roveto",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/veto1024/ts-md-boilerplate"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/veto1024"
  },
  "files": [
    "dist",
    "types"
  ],
  "unpkg": "dist/index.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "description": "Boilerplate code for a Typescript/Material Design project with Formik helpers.",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@iconify/icons-ant-design": "^1.1.0",
    "@iconify/icons-eva": "^1.1.0",
    "@iconify/icons-ic": "^1.1.10",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.47",
    "@mui/material": "^5.0.0",
    "@mui/utils": "^5.0.0",
    "date-fns": "^2.29.3",
    "dotenv": "^14.3.0",
    "formik": "^2.2.9",
    "history": "^5.0.1",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.1.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.0.0-beta.4",
    "react-scripts": "^4.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prepublishOnly": "npm run build",
    "types" : "ts-node --noEmit",
    "test": "react-scripts test",
    "build": "rm -rf dist && rollup -c",
    "build-watch": "rollup -c -w",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@iconify/icons-la": "^1.1.0",
    "@iconify/icons-tabler": "^1.1.15",
    "@iconify/react": "^3.1.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.2",
    "puppeteer": "^15.5.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2"
  }
}
