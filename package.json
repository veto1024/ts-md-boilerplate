    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0-beta.4",
    "react-scripts": "^5.0.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1"
